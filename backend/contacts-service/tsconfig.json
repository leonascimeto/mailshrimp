{
   "compilerOptions": {
     "target": "es6",
     "module": "commonjs",
     "outDir": "./dist/",
     "rootDirs": ["./src/", "../__commons__/src"],
     "strict": true,
     "esModuleInterop": true,
     "forceConsistentCasingInFileNames": true,
     "moduleResolution": "node",
     "baseUrl": ".",
     "paths": {
       "ms-commons/*": ["../__commons__/src/*"]
     },
   },
   "exclude": [
     "node_modules",
     "__tests__"
   ]
 }